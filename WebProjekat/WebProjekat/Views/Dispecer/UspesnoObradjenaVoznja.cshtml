@model WebProjekat.Models.Voznja
@{
    ViewBag.Title = "UspesnoObradjenaVoznja";
}

<html>
<body>
    <h2>Vožnja na lokaciju: @Model.LokacijaNaKojuTaxiDolazi.Adresa.Ulica @Model.LokacijaNaKojuTaxiDolazi.Adresa.Broj, @Model.LokacijaNaKojuTaxiDolazi.Adresa.NaseljenoMesto je uspešno obrađena.</h2>
    <h3>Dodeljen joj je vozač sa korisničkim imenom @Model.Vozac.KorisnickoIme.</h3>
    <form action="/Dispecer/Index">
        <input type="submit" value="Nazad" />
    </form>
</body>
</html>

